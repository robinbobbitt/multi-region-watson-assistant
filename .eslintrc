{
    "extends": "airbnb-base",

    "rules": {
        "linebreak-style": 0,
        "func-names": ["error", "never"],
        "max-len": ["warn", 170],
        "no-useless-escape": ["warn"],
        "no-continue": ["warn"],
        "no-control-regex": ["warn"],
        "no-unused-vars": ["warn"],
        "comma-dangle": 0,
        "no-console": 0,
        "no-param-reassign": [2, {
        "props": false
      }],
      "prefer-destructuring": 0
    }
}
